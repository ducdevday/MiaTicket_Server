name: dockercompose5116071195702982887
services:
  miaticket.rabbitmq:
    container_name: miaticket.rabbitmq
    environment:
      RABBITMQ_DEFAULT_PASS: mypass
      RABBITMQ_DEFAULT_USER: guest
    image: rabbitmq:3.8-management-alpine
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5672
      published: "5672"
      protocol: tcp
    - mode: ingress
      target: 15672
      published: "15672"
      protocol: tcp
  miaticket.redis:
    container_name: miaticket.redis
    image: redis:latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 6379
      published: "6379"
      protocol: tcp
    restart: always
  miaticket.webapi:
    build:
      context: E:\WorkSpace\Home\Dotnet\MiaTicket
      dockerfile: MiaTicket.WebAPI/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: miaticketwebapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 8080
      protocol: tcp
    - mode: ingress
      target: 8081
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\Ming Duc\AppData\Roaming/Microsoft/UserSecrets
      target: /home/app/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Ming Duc\AppData\Roaming/ASP.NET/Https
      target: /home/app/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose5116071195702982887_default